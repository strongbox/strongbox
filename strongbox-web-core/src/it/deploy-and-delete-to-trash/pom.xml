<project>

    <modelVersion>4.0.0</modelVersion>

    <groupId>org.carlspring.maven</groupId>
    <artifactId>test-project</artifactId>
    <version>1.0.5</version>

    <dependencies>
        <dependency>
            <groupId>org.carlspring.strongbox</groupId>
            <artifactId>strongbox-client</artifactId>
            <version>@project.version@</version>
        </dependency>

        <dependency>
            <groupId>org.carlspring.maven</groupId>
            <artifactId>maven-commons</artifactId>
            <version>@version.maven.commons@</version>
        </dependency>
    </dependencies>

    <distributionManagement>
        <repository>
            <id>releases-with-valid-user</id>
            <url>http://localhost:48080/storages/storage0/releases-with-trash/</url>
        </repository>
        <snapshotRepository>
            <id>snapshots-with-valid-user</id>
            <url>http://localhost:48080/storages/storage0/snapshots-with-trash/</url>
            <uniqueVersion>true</uniqueVersion>
        </snapshotRepository>
    </distributionManagement>

</project>
